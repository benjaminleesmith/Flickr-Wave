<?xml version="1.0" encoding="UTF-8" ?>
<Module>
  <ModulePrefs title="Latest Flickr Uploads">
    <Require feature="wave" /> 
  </ModulePrefs>
  <Content type="html">
    <![CDATA[     
		  <% @photos.each do |photo| %>
        <%= link_to(image_tag(photo.url_s, :width => photo_width(photo), :height => photo_height(photo)), photo_source(photo), :target => '_flickr') %>
		  <% end %>
    ]]>
  </Content>
</Module>